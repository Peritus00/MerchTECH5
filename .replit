name = "Stable Backend Server"
author = 42127692
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"🧹 Cleaning up any existing processes...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"node.*simple-server\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"node.*5000\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "lsof -ti:5000 | xargs kill -9 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 3"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"🚀 Starting backend server...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd server && node simple-server.js"

