[[workflows.workflow]]
name = "Clean Port Fix"
author = 42127692
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"🧹 COMPREHENSIVE PORT 5000 CLEANUP...\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"📍 Step 1: Kill ALL processes using port 5000\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"5000\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \":5000\" || true  "

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "lsof -ti:5000 | xargs kill -9 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "fuser -k 5000/tcp 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"📍 Step 2: Kill ALL node server processes\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"node.*server\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"simple-server\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"stable-server\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"📍 Step 3: Clear frontend cache completely\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rm -rf .expo/* 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rm -rf node_modules/.cache/* 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "rm -rf .next/* 2>/dev/null || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"📍 Step 4: Set correct environment variables\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "export PORT=5001"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "export EXPO_PUBLIC_API_URL=https://4311622a-238a-4013-b1eb-c601507a6400-00-3l5qvyow6auc.kirk.replit.dev:5001/api"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"📍 Step 5: Start backend on correct port 5001\""

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd server && PORT=5001 node simple-server.js"

[[workflows.workflow]]
